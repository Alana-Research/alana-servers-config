vagrant-up:
	VAGRANT_VAGRANTFILE=Vagrantfile.ubuntu20_x64 vagrant up

vagrant-ssh:
	VAGRANT_VAGRANTFILE=Vagrantfile.ubuntu20_x64 vagrant ssh

vagrant-destroy:
	VAGRANT_VAGRANTFILE=Vagrantfile.ubuntu20_x64 vagrant destroy

vagrant-reload:
	VAGRANT_VAGRANTFILE=Vagrantfile.ubuntu20_x64 vagrant reload




